<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.screens.LoadingController">
   <children>
      <TableView fx:id="mainTable" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
        <columns>
            <TableColumn fx:id="winnerNameColumn" editable="false" prefWidth="100.0" resizable="false" text="Победитель" />
            <TableColumn fx:id="dateColumn" editable="false" prefWidth="100.0" resizable="false" text="Дата" />
            <TableColumn fx:id="loadBtnColumn" editable="false" prefWidth="100.0" resizable="false" />
        </columns>
      </TableView>
   </children>
</VBox>
