   <children>
      <TableView fx:id="mainTable" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
        <columns>
            <TableColumn fx:id="nameColumn" editable="false" prefWidth="100.0" resizable="false" text="Название" />
            <TableColumn fx:id="multiplierColumn" editable="false" prefWidth="100.0" resizable="false" text="Сумма" />
        </columns>
      </TableView>
   </children>
</VBox>
